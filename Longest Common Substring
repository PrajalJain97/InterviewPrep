#include<bits/stdc++.h>

using namespace std;

int findLCS(string &a,string &b)
{
    int n=a.size(),m=b.size(),LCS=0;
    if(n==0 || m==0)
        return 0;

    vector< vector<int> > dp(2,vector<int>(m+1,0));

    for(int i=0;i<m+1;i++)
        dp[0][i]=0;

    for(int i=1;i<n+1;i++)
    {
        for(int j=1;j<m+1;j++)
        {
           if(a[i-1] == b[j-1])
           {
              dp[i%2][j] = dp[(i-1)%2][j-1]+1;
              LCS=max(LCS,dp[i%2][j]);
           }

           else
              dp[i%2][j] = 0;
        }
    }
    return LCS;
}
int main()
{
    int n,m;
    string a,b;
    a="AXBYCDE";
    b="AXBYCVNDE";
    int ans=findLCS(a,b);
    cout<<ans;
    return 0;
}

